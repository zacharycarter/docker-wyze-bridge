<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Name>docker-wyze-bridge</Name>
  <Repository>zacharycarter/wyze-bridge</Repository>
  <Registry>https://hub.docker.com/r/zacharycarter/wyze-bridge</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/zacharycarter/docker-wyze-bridge</Support>
  <Project>https://github.com/zacharycarter/docker-wyze-bridge</Project>
  <Overview>RTMP/RTSP/HLS bridge for Wyze cams in a docker container</Overview>
  <Category>Home Automation:</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon/>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Description>RTMP/RTSP/HLS bridge for Wyze cams in a docker container</Description>
  <Networking>
    <Mode>bridge</Mode>
  </Networking>
  <Data/>
  <Environment>
    <Variable>
      <Value/>
      <Name>WYZE_EMAIL</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>WYZE_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>NET_MODE</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="WYZE_EMAIL" Target="WYZE_EMAIL" Default="" Mode="" Description="Container Variable: WYZE_EMAIL" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="WYZE_PASSWORD" Target="WYZE_PASSWORD" Default="" Mode="" Description="Container Variable: WYZE_PASSWORD" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="NET_MODE" Target="NET_MODE" Default="ANY" Mode="" Description="Container Variable: NET_MODE" Type="Variable" Display="always" Required="false" Mask="false">Any</Config>
  <Config Name="RTSP Port" Target="8554" Default="8554" Mode="tcp" Description="RTSP Port" Type="Port" Display="always" Required="true" Mask="false">8554</Config>
  <Config Name="RTMP Port" Target="1935" Default="1935" Mode="tcp" Description="RTMP Port" Type="Port" Display="always" Required="true" Mask="false">1937</Config>
  <Config Name="HLS Port" Target="8888" Default="8888" Mode="tcp" Description="HLS Port" Type="Port" Display="always" Required="true" Mask="false">8888</Config>
</Container>
